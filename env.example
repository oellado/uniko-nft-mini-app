# üåê UNIK≈å NFT ENVIRONMENT CONFIGURATION
# Copy this file to .env and update the values

# NETWORK CONFIGURATION
# Set to 'mainnet' for production, 'testnet' for development
NEXT_PUBLIC_NETWORK=testnet

# CONTRACT ADDRESSES
# These will be automatically selected based on NEXT_PUBLIC_NETWORK
NEXT_PUBLIC_TESTNET_CONTRACT=0xE8dbA1b91c3D59295a9937E666Ee309E793Eaf7E
NEXT_PUBLIC_MAINNET_CONTRACT=0x[YOUR_MAINNET_CONTRACT_ADDRESS]

# WALLET CONNECT (if needed)
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_project_id_here

# ALCHEMY/INFURA KEYS (if needed)
NEXT_PUBLIC_ALCHEMY_KEY=your_alchemy_key_here
NEXT_PUBLIC_INFURA_KEY=your_infura_key_here

# DEPLOYMENT SETTINGS
DEPLOYER_PRIVATE_KEY=your_private_key_for_deployment
ETHERSCAN_API_KEY=your_etherscan_api_key_for_verification

# Deployment Configuration
PRIVATE_KEY=your_wallet_private_key_here
BASESCAN_API_KEY=your_basescan_api_key_here

# RPC URLs (optional - using defaults)
BASE_RPC_URL=https://mainnet.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# Contract Configuration
MINT_PRICE=0.001
MAX_SUPPLY=10000

# Frame Configuration
FRAME_URL=http://localhost:5179 